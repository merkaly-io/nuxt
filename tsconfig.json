{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "baseUrl": "./",
    "strict": true,
    "esModuleInterop": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "@types/node",
      "@merkaly/sdk-js",
      "@nuxt/types",
      "@types/auth0-lock"
    ]
  },
  "exclude": [
    "nuxt.config.ts"
  ]
}
