{
  "name": "@merkaly/nuxt",
  "version": "0.2.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/merkaly-io/nuxt.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "build:stub": "nuxt-module-build build --stub",
    "dev": "pnpm dev:prepare && cd playground && pnpm storybook",
    "dev:build": "nuxi build playground",
    "dev:prepare": "pnpm build:stub && nuxi prepare playground",
    "lint": "eslint .",
    "prepack": "pnpm build",
    "prepare": "nuxt-module-build prepare",
    "release": "pnpm lint && pnpm test && pnpm build && changelogen --release && npm publish --access public && git push --follow-tags",
    "test": "vitest run",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.12.0",
    "@bootstrap-vue-next/nuxt": "^0.42.0",
    "@nuxt/devtools": "^3.1.0",
    "@nuxt/eslint": "1.13.0",
    "@nuxt/eslint-config": "^1.13.0",
    "@nuxt/fonts": "0.13.0",
    "@nuxt/image": "^2.0.0",
    "@nuxt/kit": "^4.3.0",
    "@nuxtjs/plausible": "^2.0.1",
    "@types/node": "latest",
    "@types/vue-select": "^3.16.8",
    "@vueuse/components": "^14.1.0",
    "@vueuse/core": "^14.2.0",
    "@vueuse/integrations": "^14.2.0",
    "@vueuse/nuxt": "^14.2.0",
    "bootstrap": "^5.3.8",
    "bootstrap-vue-next": "^0.42.0",
    "change-case": "^5",
    "eslint": "^9.39.1",
    "filesize": "^11.0.2",
    "nuxt": "^4.3.0",
    "reflect-metadata": "^0.2.2",
    "sass": "^1.97.0",
    "typescript": "~5.9.3",
    "v-money3": "^3.24.1",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.5.22",
    "vue-router": "^5.0.0",
    "vue-select": "4.0.0-beta.6"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.3.0",
    "@nuxt/test-utils": "^3.23.0",
    "changelogen": "^0.6.2",
    "eslint-plugin-storybook": "^10.1.11",
    "vitest": "^4.0.6",
    "vue-tsc": "^3.2.1"
  },
  "optionalDependencies": {
    "defu": "^6.1.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
